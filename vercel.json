{
  "framework": "vite",
  "buildCommand": "npm run build:vercel",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "env": {
    "DEPLOY_ENV": "VERCEL"
  },
  "routes": [
    { "src": "/KhGCEtTRxy.txt", "dest": "/KhGCEtTRxy.txt" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/" }
  ]
} 